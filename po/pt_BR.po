msgid "Data length:"
msgstr "Comprimento de dados:"

#: ../data/gpm-statistics.ui.h:3 ../src/gpm-statistics.c:206
msgid "Details"
msgstr "Detalhes"

#: ../data/gpm-statistics.ui.h:4
