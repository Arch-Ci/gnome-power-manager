#: ../src/gpm-statistics.c:1655 ../src/gpm-statistics.c:1661
#: ../src/gpm-statistics.c:1667
msgid "Cell charge"
msgstr "セルの充電"

#. TRANSLATORS: this is the Y axis on the graph
#: ../src/gpm-statistics.c:1612 ../src/gpm-statistics.c:1618
msgid "Predicted time"
msgstr "予定時間"

#. TRANSLATORS: this is the Y axis on the graph
