#: ../src/gpm-backlight-helper.c:167
msgid "Get the number of brightness levels supported"
msgstr "Obtener el númberu de niveles de brillu sofitaos"

#. TRANSLATORS: tool that is used when copying profiles system-wide
#: ../src/gpm-backlight-helper.c:182
msgid "GNOME Power Manager Backlight Helper"
msgstr "Encontu de la lluz de fondu del Xestor d'enerxía GNOME"

#. TRANSLATORS: user did not specify valid options
#: ../src/gpm-backlight-helper.c:190
msgid "No valid option was specified"
msgstr "Nun s'especificó denguna opción válida"

#. TRANSLATORS: no backlights found
#: ../src/gpm-backlight-helper.c:199
msgid "No backlights were found on your system"
msgstr "Nun s'alcontró denguna lluz de fondu nel sistema"

#. TRANSLATORS: failed to access backlight file
#: ../src/gpm-backlight-helper.c:210
msgid "Could not get the value of the backlight"
msgstr "Nun pudo obtenese'l valor de la lluz de fondu"

#. TRANSLATORS: failed to access backlight file
#: ../src/gpm-backlight-helper.c:228
msgid "Could not get the maximum value of the backlight"
msgstr "Nun pudo obtenese'l valor máximu de la lluz de fondu"

#. TRANSLATORS: only able to install profiles as root
#: ../src/gpm-backlight-helper.c:245
msgid "This program can only be used by the root user"
msgstr "Esti programa namái puede usalu'l superusuariu"

#. TRANSLATORS: the program must never be directly run
#: ../src/gpm-backlight-helper.c:254
msgid "This program must only be run through pkexec"
msgstr "Esti programa namái tien d'usase a traviés de pkexec"

#. TRANSLATORS: failed to access backlight file
#: ../src/gpm-backlight-helper.c:265
msgid "Could not set the value of the backlight"
msgstr "Nun pudo afitase'l valor de la lluz de fondu"

#: ../src/gpm-common.c:55
msgid "Unknown time"
msgstr "Tiempu desconocíu"

#: ../src/gpm-common.c:60
#, c-format
