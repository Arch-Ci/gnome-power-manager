msgstr "Istorijat uređaja"

#. TRANSLATORS: shown on the titlebar
#: ../src/gpm-statistics.c:1400
msgid "Device Profile"
msgstr "Profil uređaja"

#. TRANSLATORS: shown on the titlebar
#: ../src/gpm-statistics.c:1402
msgid "Processor Wakeups"
