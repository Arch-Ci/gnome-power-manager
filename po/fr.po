#: ../src/gpm-main.c:355 ../src/gpm-main.c:374
#, c-format
msgid ""
"You have approximately <b>%s</b> of remaining battery life (%i%%). Plug in "
"your AC Adapter to avoid losing data."
msgstr ""
"Il reste environ <b>%s</b> de temps sur la batterie (%i%%). Veuillez "
"brancher votre adaptateur secteur pour éviter de perdre des données."

#: ../src/gpm-main.c:359
msgid "Battery Critically Low"
msgstr "Niveau critique de la batterie"

#: ../src/gpm-main.c:377
msgid "Battery Low"
msgstr "Batterie faible"

#: ../src/gpm-main.c:423
msgid "AC Power Unplugged"
msgstr "Prise secteur débranchée"

#: ../src/gpm-main.c:424
msgid "The AC Power has been unplugged. The system is now using battery power."
msgstr ""
"La prise secteur est débranchée. Le système utilise maintenant la batterie."

#: ../src/gpm-main.c:502
msgid "Battery Charged"
msgstr "Batterie chargée"

#: ../src/gpm-main.c:502
msgid "Your battery is now fully charged"
msgstr "La batterie est maintenant entièrement chargée"

#: ../src/gpm-notification.c:219
msgid "charging"
msgstr "en charge"

#: ../src/gpm-notification.c:221
msgid "discharging"
msgstr "en décharge"

#: ../src/gpm-notification.c:224
msgid "charged"
msgstr "chargé"

#: ../src/gpm-notification.c:239
msgid "until charged"
msgstr "avant chargement"

#: ../src/gpm-notification.c:242
msgid "until empty"
msgstr "avant épuisement"

#: ../src/gpm-notification.c:309
msgid "Computer is running on battery power\n"
msgstr "L'ordinateur est branché sur la batterie\n"

#: ../src/gpm-notification.c:311
msgid "Computer is running on AC power\n"
msgstr "L'ordinateur est branché sur le secteur\n"

