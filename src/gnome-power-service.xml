<?xml version="1.0" encoding="UTF-8" ?>

<node name="/">
  <interface name="org.gnome.GnomePowerManager">

    <method name="isUserIdle">
      <arg type="b" direction="out"/>
    </method>

    <method name="isOnBattery">
      <arg type="b" direction="out"/>
    </method>

    <method name="isOnUps">
      <arg type="b" direction="out"/>
    </method>

    <method name="isOnAc">
      <arg type="b" direction="out"/>
    </method>

    <method name="ActionRegister">
      <arg type="i"/>
      <arg type="s"/>
      <arg type="b" direction="out"/>
    </method>

    <method name="ActionUnregister">
      <arg type="i"/>
      <arg type="b" direction="out"/>
    </method>

    <method name="Ack">
      <arg type="i"/>
      <arg type="b" direction="out"/>
    </method>

    <method name="Nack">
      <arg type="i"/>
      <arg type="s"/>
      <arg type="b" direction="out"/>
    </method>

<!--
    <method name="Wait">
      <arg type="i"/>
      <arg type="i"/>
      <arg type="s"/>
      <arg type="b" direction="out"/>
    </method>
-->

    <signal name="mainsStatusChanged"/>
    <signal name="performingAction"/>
<!--
    <signal name="userIdleStatusChanged"/>
-->
    <signal name="actionAboutToHappen"/>

  </interface>
</node>
