<?xml version="1.0" encoding="UTF-8" ?>

<node name="/">
  <interface name="net.sf.PowerManagerInterface">

    <method name="isUserIdle">
      <arg type="b" direction="out"/>
    </method>

    <method name="isRunningOnMains">
      <arg type="b" direction="out"/>
    </method>

    <method name="isActive">
      <arg type="b" direction="out"/>
    </method>

    <method name="ActionRegister">
      <arg type="i"/>
      <arg type="s"/>
      <arg type="b" direction="out"/>
    </method>

    <method name="ActionUnregister">
      <arg type="i"/>
      <arg type="b" direction="out"/>
    </method>

    <method name="Ack">
      <arg type="i"/>
      <arg type="b" direction="out"/>
    </method>

    <method name="Nack">
      <arg type="i"/>
      <arg type="s"/>
      <arg type="b" direction="out"/>
    </method>

    <method name="Wait">
      <arg type="i"/>
      <arg type="i"/>
      <arg type="s"/>
      <arg type="b" direction="out"/>
    </method>

    <signal name="mainsStatusChanged"/>
    <signal name="userIdleStatusChanged"/>
    <signal name="actionAboutToHappen"/>
    <signal name="performingAction"/>

  </interface>
</node>
