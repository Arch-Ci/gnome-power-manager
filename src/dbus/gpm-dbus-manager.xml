    <method name="Shutdown">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_shutdown_dbus_method"/>
    </method>
    <method name="Reboot">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_reboot_dbus_method"/>
    </method>
