<?xml version="1.0" encoding="UTF-8"?>
<node name="/">
  <interface name="org.gnome.PowerManager">
    <method name="GetLowPowerMode">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_get_low_power_mode"/>
      <arg type="b" name="low_power" direction="out"/>
    </method>
    <method name="GetOnAc">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_get_on_ac"/>
      <arg type="b" name="on_ac" direction="out"/>
    </method>
    <method name="AllowedSuspend">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_allowed_suspend"/>
      <arg type="b" name="can" direction="out"/>
    </method>
    <method name="AllowedHibernate">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_allowed_hibernate"/>
      <arg type="b" name="can" direction="out"/>
    </method>
    <method name="AllowedShutdown">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_allowed_shutdown"/>
      <arg type="b" name="can" direction="out"/>
    </method>
    <method name="AllowedReboot">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="gpm_manager_allowed_reboot"/>
      <arg type="b" name="can" direction="out"/>
    </method>
