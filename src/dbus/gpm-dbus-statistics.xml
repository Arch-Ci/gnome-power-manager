<?xml version="1.0" encoding="UTF-8"?>
<node name="/">
  <interface name="org.gnome.PowerManager">
    <method name="GetTypes">
      <arg type="as" name="types" direction="out"/>
    </method>

    <method name="GetAxisType">
      <arg type="s" name="type" direction="in"/>
      <arg type="s" name="axis_type_x" direction="out"/>
      <arg type="s" name="axis_type_y" direction="out"/>
    </method>

    <method name="GetData">
      <arg type="i" name="seconds" direction="in"/>
      <arg type="s" name="type" direction="in"/>
      <arg type="a(iii)" name="data" direction="out"/>
    </method>

    <method name="GetEventLog">
      <arg type="i" name="seconds" direction="in"/>
      <arg type="a(ii)" name="data" direction="out"/>
    </method>
  </interface>
</node>
