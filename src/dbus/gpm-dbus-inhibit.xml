<?xml version="1.0" encoding="UTF-8"?>
<node name="/">
  <interface name="org.gnome.PowerManager.Inhibit">
     <method name="InhibitAuto">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="s" name="application" direction="in"/>
      <arg type="s" name="reason" direction="in"/>
      <arg type="u" name="cookie" direction="out"/>
    </method>
     <method name="InhibitManual">
      <annotation name="org.freedesktop.DBus.GLib.Async" value=""/>
      <arg type="s" name="application" direction="in"/>
      <arg type="s" name="reason" direction="in"/>
      <arg type="u" name="cookie" direction="out"/>
    </method>
    <method name="UnInhibit">
      <arg type="u" name="cookie" direction="in"/>
    </method>
    <method name="IsValid">
      <arg type="b" name="user_action" direction="in"/>
      <arg type="b" name="valid" direction="out"/>
    </method>
    <method name="GetRequests">
      <arg type="ass" name="requests" direction="out"/>
    </method>
<!--
    <signal name="IsValidChanged"/>
-->
  </interface>
</node>
